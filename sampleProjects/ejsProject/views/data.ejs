<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Users data</title>
  </head>
  <body>
    <table>
      <thead>
        <tr>
          <th>Name</th>
          <th>E-mail</th>
          <th>Address</th>
        </tr>
      </thead>
      <tbody>
        <% data.forEach(entry => { %>
        <tr>
          <td><%= entry.name %></td>
          <td><%= entry.email %></td>
          <td><%= entry.address %></td>
        </tr>
        <% }) %>
      </tbody>
    </table>
    <a href="/export">Export as CSV</a>
  </body>
</html>
